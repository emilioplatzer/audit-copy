{
  "name": "audit-copy",
  "description": "copy a JS Object for latter audit and detect modifications",
  "version": "0.0.3",
  "author": "Emilio Platzer <emilioplatzer@gmail.com>",
  "repository": "emilioplatzer/audit-copy",
  "license": "MIT",
  "main": "audit-copy.js",
  "files": [
    "audit-copy.js"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "expect.js": "~0.3.1",
    "istanbul": "~0.4.3",
    "mocha": "~2.4.5"
  },
  "engines": {
    "node": ">= 0.12"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "test-nc": "mocha --reporter spec --bail --check-leaks --no-colors test/"
  },
  "qa-control": {
    "package-version": "0.3.0",
    "coverage": 90,
    "run-in": "server",
    "stability": "extending",
    "type": "lib"
  }
}
